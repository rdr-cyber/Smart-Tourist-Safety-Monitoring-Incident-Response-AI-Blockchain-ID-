{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "preview.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/$",
      "dest": "/index.html"
    },
    {
      "src": "/preview$",
      "dest": "/preview.html"
    }
  ]
}